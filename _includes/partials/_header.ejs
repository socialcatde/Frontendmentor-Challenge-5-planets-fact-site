<nav>
  <div class="headline">
    <h1><%- title %></h1>
    <button class="hamburger hamburger--squeeze" type="button">
      <span class="hamburger-box">
        <span class="hamburger-inner"></span>
      </span>
    </button>
  </div>
  <ul class="nav_list">
    <% for (planet of planets) {%>
    <li>
      <div class="planetNav">
        <span
          class="planet-menu"
          id="span<%= planet.name.toLowerCase() %>"
          aria-hidden="true"
        ></span>
        <a
          id="<%= planet.name.toLowerCase() %>"
          href="/<%= planet.name.toLowerCase() %>"
          ><%= planet.name %></a
        >
      </div>
      <span class="arrow"
        ><img
          src="/img/icon-chevron.svg"
          aria-hidden="true"
          alt="Navigation Arrow"
      /></span>
      <% } %>
    </li>
  </ul>
</nav>
